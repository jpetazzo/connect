{
  "name": "redi-connect-front",
  "version": "0.1.0",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./redi-connect-front/node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@date-io/moment": "1.3.11",
    "@material-ui/core": "4.5.0",
    "@material-ui/icons": "4.4.3",
    "@material-ui/pickers": "3.2.6",
    "@types/react-select": "^3.0.13",
    "@types/react-slick": "^0.23.4",
    "axios": "^0.19.0",
    "bulma-checkradio": "^1.1.1",
    "clsx": "^1.0.4",
    "final-form": "^4.18.5",
    "final-form-material-ui": "^0.3.0",
    "formik": "^2.1.4",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-http-backend": "^1.0.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "react": "16.13.1",
    "react-bulma-components": "^3.2.0",
    "react-dom": "16.13.1",
    "react-final-form": "^6.3.0",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.1.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.1.2",
    "react-s3-uploader": "^4.8.0",
    "react-select": "^3.1.0",
    "react-slick": "^0.26.1",
    "redux": "^4.0.4",
    "redux-observable": "1.2.0",
    "rxjs": "^6.5.2",
    "slick-carousel": "^1.8.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.142",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "7.1.4",
    "@types/react-router": "5.1.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "0.26.24",
    "@welldone-software/why-did-you-render": "3.3.7",
    "babel-loader": "^8.0.6",
    "cz-conventional-changelog": "^3.0.2",
    "env-cmd": "^10.0.1",
    "husky": "3.0.8",
    "prettier": "^1.18.2",
    "react-scripts": "3.4.1",
    "semantic-release": "^15.13.24",
    "typescript": "3.9.5",
    "utility-types": "^3.8.0",
    "yarn-check": "^0.0.3"
  },
  "scripts": {
    "start:berlin": "REACT_APP_REDI_LOCATION=berlin react-scripts start",
    "start:munich": "REACT_APP_REDI_LOCATION=munich react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:autofix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "invalidate-cache:staging": "aws cloudfront create-invalidation --distribution-id E1HRU1RBE5MXQE --paths '/*' --profile redi-connect-deployment",
    "invalidate-cache:prod:berlin": "aws cloudfront create-invalidation --distribution-id E3MVMTF8ANMDW --paths '/*' --profile redi-connect-deployment",
    "invalidate-cache:prod:munich": "aws cloudfront create-invalidation --distribution-id E94A2JJ5XRS3M --paths '/*' --profile redi-connect-deployment",
    "invalidate-cache:demonstration": "aws cloudfront create-invalidation --distribution-id E11DMK1GIVHK3G --paths '/*' --profile redi-connect-deployment",
    "deploy-staging": "REACT_APP_REDI_LOCATION=berlin env-cmd -f .env.staging yarn build && aws s3 rm s3://app.staging.connect.redi-school.org/ --recursive --profile redi-connect-deployment --region eu-west-1 && aws s3 sync build/ s3://app.staging.connect.redi-school.org --delete --acl public-read --profile redi-connect-deployment --region eu-west-1 && yarn invalidate-cache:staging",
    "deploy-production": "yarn deploy-production:berlin && yarn deploy-production:munich",
    "deploy-production:berlin": "REACT_APP_REDI_LOCATION=berlin env-cmd -f .env.production yarn build && aws s3 rm s3://connect.redi-school.org/ --recursive --profile redi-connect-deployment --region eu-west-1 && aws s3 sync build/ s3://connect.redi-school.org --delete --acl public-read --profile redi-connect-deployment --region eu-west-1 && yarn invalidate-cache:prod:berlin",
    "deploy-production:munich": "REACT_APP_REDI_LOCATION=munich env-cmd -f .env.production yarn build && aws s3 rm s3://connect.munich.redi-school.org/ --recursive --profile redi-connect-deployment --region eu-west-1 && aws s3 sync build/ s3://connect.munich.redi-school.org --delete --acl public-read --profile redi-connect-deployment --region eu-west-1 && yarn invalidate-cache:prod:munich",
    "deploy-demonstration": "REACT_APP_REDI_LOCATION=munich env-cmd -f .env.demonstration yarn build && aws s3 rm s3://app.demo.connect.redi-school.org/ --recursive --profile redi-connect-deployment --region eu-west-1 && aws s3 sync build/ s3://app.demo.connect.redi-school.org --delete --acl public-read --profile redi-connect-deployment --region eu-west-1 && yarn invalidate-cache:demonstration",
    "check-versions": "yarn-check -u",
    "commit": "npx git-cz",
    "release": "npx semantic-release"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
